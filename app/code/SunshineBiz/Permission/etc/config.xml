            </permission_setup>
        </resources>
        <di>
            <preferences>
        	<Mage_User_Model_Resource_User>SunshineBiz_Permission_Model_Resource_User</Mage_User_Model_Resource_User>
        	<Mage_User_Model_Acl_Loader_Role>SunshineBiz_Permission_Model_Acl_Loader_Role</Mage_User_Model_Acl_Loader_Role>
