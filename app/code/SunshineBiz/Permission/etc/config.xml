        </routers>
    </admin>
    <adminhtml>
    	<translate>
            <modules>
                <SunshineBiz_Permission>
                    <files>
                        <default>SunshineBiz_Permission.csv</default>
                    </files>
                </SunshineBiz_Permission>
            </modules>
        </translate>
        <layout>
            <updates>
                <permission module="SunshineBiz_Permission">
                    <file>layout.xml</file>
                </permission>
            </updates>
        </layout>
    </adminhtml>
